{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "test:prettier": "prettier --config .prettierrc --check \"**/*.{js,jsx,ts,tsx}\"",
    "test:typescript": "npx tsc --noEmit",
    "checks": "yarn lint && yarn test:prettier && yarn test:typescript",
    "checks:fix": "npx tsc --noEmit && npx eslint --fix . && npx prettier --write ."
  },
  "dependencies": {
    "next": "14.2.4",
    "react": "18.3",
    "react-dom": "18.3",
    "react-hook-form": "7.51.5",
    "react-icons": "5.2.1",
    "react-slick": "0.30.2",
    "resend": "3.2.0",
    "slick-carousel": "1.8.1"
  },
  "devDependencies": {
    "@types/node": "20",
    "@types/react": "18.3",
    "@types/react-dom": "18.3",
    "@types/react-slick": "0.23.13",
    "autoprefixer": "10.4.19",
    "eslint": "8",
    "eslint-config-next": "14.2.4",
    "husky": "9.0.11",
    "postcss": "8",
    "prettier": "3.3.2",
    "prettier-plugin-tailwindcss": "0.6.3",
    "tailwindcss": "3.4.4",
    "typescript": "5"
  }
}
